{
  "name": "node_boilerplate_typescript",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=920 build/index.js",
    "start:dev": "npx nodemon ./src/index.ts",
    "build": "rimraf ./build && tsc",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "typeorm:dev": "npx cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js",
    "typeorm:test": "npx cross-env NODE_ENV=test ts-node ./node_modules/typeorm/cli.js",
    "db:update": "npm run build && npm run typeorm migration:run",
    "db:update:dev": "npm run typeorm:dev migration:run",
    "db:update:test": "npm run typeorm:test migration:run",
    "ci:db:update": "npm run typeorm migration:run",
    "db:generate:dev": "npm run typeorm:dev migration:generate -- -n",
    "test": "npx cross-env NODE_ENV=test mocha --exit --timeout 15000 -r ts-node/register --file ./test/setup.ts './test/**/*.test.ts'",
    "test:watch": "npx cross-env NODE_ENV=test mocha --timeout 15000 -r ts-node/register -w -R min --file ./test/setup.ts './test/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "Sandeep Singh",
  "engines": {
    "node": "16.x"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/mocha": "^9.0.0",
    "@types/newrelic": "^7.0.2",
    "@types/sinon": "^9.0.11",
    "acl": "^0.4.11",
    "acl-mem-regexp": "^0.2.0",
    "aws-sdk": "^2.1137.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-validator": "^6.14.0",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "newrelic": "^8.6.0",
    "os": "^0.1.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.24.0",
    "swagger-ui-express": "^4.2.0",
    "throng": "^5.0.0",
    "tsoa": "^3.14.1",
    "typeorm": "^0.2.39",
    "uuid": "^3.0.1"
  }
}
